doctype html
head
  meta(charset='UTF-8')
  title #{pageTitle}
  script(src='http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js')
  script(src='http://code.highcharts.com/highcharts.js')
  
#container(style='width: 550px; height: 400px; margin: 0 auto')
body


  a(href=`/uploadFile/task1/${pageTitle}`) Task1
  a(href=`/uploadFile/task2/${pageTitle}`) Task2
  a(href=`/uploadFile/task3/${pageTitle}`) Task3
  a(href=`/uploadFile/task4/${pageTitle}`) Task4
  a(href=`/uploadFile/task5/${pageTitle}`) Task5
  a(href=`/uploadFile/core1/${pageTitle}`) Core1
  a(href=`/uploadFile/core2/${pageTitle}`) Core2
  a(href=`/uploadFile/core3/${pageTitle}`) Core3
  a(href=`/uploadFile/core4/${pageTitle}`) Core4
  a(href=`/uploadFile/core5/${pageTitle}`) Core5
  

  script(language='JavaScript').
    $(document).ready(function () {

    
    



    var title = {
    text: "#{pageTitle} - #{Tc} 대한 #{d}별 능력",
    };
    var subtitle = {
    text: "20180869 전종훈",
    };
    var xAxis = {
    categories: ["#{miniTitle}1", "#{miniTitle}2", "#{miniTitle}3", "#{miniTitle}4", "#{miniTitle}5"],
    };
    var yAxis = {
    title: {
    text: "(숫자)",
    },
    plotLines: [
    {
    value: 0,
    width: 1,
    color: "#808080",
    },
    ],
    };
    var tooltip = {
    valueSuffix: "",
    };
    var legend = {
    layout: "vertical",
    align: "right",
    verticalAlign: "middle",
    borderWidth: 0,
    };
    var series = [
    {
    name: "Max",
    data: [#{maxArr}],
    },
    {
    name: "MIn",
    data: [#{minArr}],
    },
    {
    name: "average",
    data: [#{avgArr}],
    },
    ];
    var json = {};
    json.title = title;
    json.subtitle = subtitle;
    json.xAxis = xAxis;
    json.yAxis = yAxis;
    json.tooltip = tooltip;
    json.legend = legend;
    json.series = series;
    $("#container").highcharts(json);
    });